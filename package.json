{
  "name": "bs-example-html5",
  "version": "1.0.0",
  "description": "Example of how to build a BrightSign HTML5 App without using BrightAuthor:connected",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "clean": "rm -rf dist",
    "reinstall": "npm run clean && rm -rf node_modules && npm install",
    "watch": "webpack --watch",
    "put": "npm run build && ./scripts/put",
    "put:prod": "npm run build:prod && ./scripts/put"
  },
  "author": "Greg Herlein <gherlein@brightsign.biz>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@webpack-cli/generators": "^3.0.2",
    "babel-loader": "^9.1.3",
    "html-webpack-plugin": "^5.5.3",
    "webpack": "^5.82.0",
    "webpack-cli": "^5.1.0"
  },
  "engines": {
    "node": "<14.7.6"
  },
  "dependencies": {
    "node": "^14.17.6",
    "nvm": "^0.0.4"
  }
}
